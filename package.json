{
    "name": "@doomcommerce/shopify-search",
    "version": "1.2.0",
    "description": "Shopify search query builder",
    "main": "./Build/Source/mod.js",
    "types": "./Build/Source/mod.d.ts",
    "exports": {
        ".": {
            "require": "./Build/Source/mod.js",
            "types": "./Build/Source/mod.d.ts",
            "default": "./Build/Source/mod.js"
        }
    },
    "scripts": {
